#include <stdio.h>

void main()
{      //Intializing the array
  int arr1[3][3],i,j;
  
       printf("To display an array of size 3x3  \n");
    //Store values in the array
       printf("Enter the input elements in the matrix :\n");
  for(i=0;i<3;i++)
  {
      for(j=0;j<3;j++)
      {
	      printf("element - [%d],[%d] : ",i,j);
	      scanf("%d",&arr1[i][j]);
      }
  }  
 
 printf("\nThe matrix is : \n");
  for(i=0;i<3;i++)
  {
      printf("\n");
      for(j=0;j<3;j++)
           printf("%d\t",arr1[i][j]);
  }
 printf("\n");  //Displaying the output
}
